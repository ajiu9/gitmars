{
    "extends": "../../tsconfig.json",
    "exclude": ["__tests__", "../../temp"],
    "include": ["**/*.ts", "typings"],
    "compilerOptions": {
        "removeComments": true,
        "declaration": true,
        "emitDeclarationOnly": true,
        "target": "ESNext",
        // "outDir": "dist",
        "module": "ESNext",
        "lib": ["ESNext"],
        "sourceMap": false,
        "baseUrl": ".",
        "paths": {
            "#lib/*": ["./src/*"],
            "#conf/*": ["./src/conf/*"]
        }
    }
}
