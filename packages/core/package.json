{
    "name": "@gitmars/core",
    "description": "gitmars核心程序",
    "version": "3.0.0-alpha.12",
    "private": false,
    "main": "./lib/index.js",
    "module": "./es/index.js",
    "exports": {
        ".": {
            "require": "./lib/index.js",
            "import": "./es/index.mjs",
            "types": "./es/index.d.ts"
        },
        "./es": "./es/index.mjs",
        "./lib": "./lib/index.js",
        "./es/*.mjs": "./es/*.mjs",
        "./es/*": "./es/*.mjs",
        "./lib/*.js": "./lib/*.js",
        "./lib/*": "./lib/*.js",
        "./*": {
            "require": "./lib/*.js",
            "import": "./es/*.mjs",
            "types": "./es/*.d.ts"
        }
    },
    "files": [
        "cache/.gitkeep",
        "es",
        "lib",
        "sh",
        "typings",
        "*.d.ts"
    ],
    "scripts": {},
    "directories": {
        "bin": "bin",
        "es": "es",
        "lib": "lib",
        "src": "src",
        "cache": "cache",
        "sh": "sh",
        "typings": "typings"
    },
    "dependencies": {
        "@jssj/request": "^1.0.0-alpha.8",
        "ci-info": "^3.3.0",
        "colors": "^1.4.0",
        "columnify": "^1.6.0",
        "commander": "^9.2.0",
        "cosmiconfig": "^7.0.1",
        "cross-spawn": "^7.0.3",
        "dayjs": "^1.11.1",
        "debug": "^4.3.4",
        "inquirer": "^8.2.4",
        "js-cool": "^2.3.2",
        "node-apollo": "^1.2.1",
        "ora": "^5.4.1",
        "shelljs": "^0.8.5",
        "slash": "^3.0.0"
    },
    "devDependencies": {
        "@eslint-sets/eslint-config-simple-ts": "^2.1.5",
        "@saqqdy/prettier-config": "^1.1.2",
        "@types/cross-spawn": "^6.0.2",
        "@types/inquirer": "^8.2.1",
        "@types/node": "^17.0.31",
        "@types/shelljs": "^0.8.11",
        "cron-parser": "^4.4.0",
        "cross-env": "^7.0.3",
        "eslint": "^8.14.0",
        "madge": "^5.0.1",
        "prettier": "^2.6.2",
        "tsnd": "^1.0.2",
        "typescript": "^4.6.4"
    },
    "sideEffects": false,
    "keywords": [
        "gitmars",
        "git",
        "tool"
    ],
    "license": "GPL-3.0",
    "author": "saqqdy <https://github.com/saqqdy>",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/saqqdy/gitmars.git",
        "directory": "packages/core"
    },
    "bugs": {
        "url": "https://github.com/saqqdy/gitmars/issues"
    },
    "homepage": "https://github.com/saqqdy/gitmars/tree/master/packages/core#readme",
    "publishConfig": {
        "registry": "https://registry.npmjs.org",
        "access": "public"
    }
}
